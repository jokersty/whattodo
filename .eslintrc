{
    "root": true,
    "env": {
        "node": true
    },
    "extends": ["react-app", "react-app/jest"],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "plugins": ["prettier"],
    "rules": {
        "no-console": "off",
        "no-debugger": "off",
        "@typescript-eslint/no-explicit-any": ["off"],
        "indent": ["warn", 4]
    }
}
